net: "models/train/synthetic/train.prototxt"
base_lr: 0.00002
lr_policy: "step"
gamma: 0.5
stepsize: 500
display: 500
max_iter: 10000
momentum: 0.9
weight_decay: 0.0005
snapshot: 5000
snapshot_prefix: "models/train/caffemodel/synthetic"
snapshot_after_train: false
# uncomment the following to default to CPU mode solving
# solver_mode: CPU
